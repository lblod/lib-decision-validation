                                    <div property="prov:generated" typeof=”besluit:Besluit” resource="https://data.aalter.be/id/besluiten/24.0508.3708.7968" >
                                        <span property="eli:title eli:description" datatype="xsd:string" content="2024_GR_00099 - Bezorgdheden deelkernen aanpakken. Deelkern Maria Aalter. Goedkeuring."></span>
                                        <span property="besluit:citeeropschrift" datatype="xsd:string" content="2024_GR_00099"></span>
                                    </div>
                                    <span property="openbaar" datatype="xsd:boolean" content="true"></span>
                                    <span property="dc:subject" typeof="besluit:Agendapunt" resource="https://data.aalter.be/id/agendapunten/24.0508.3708.7968"></span>
