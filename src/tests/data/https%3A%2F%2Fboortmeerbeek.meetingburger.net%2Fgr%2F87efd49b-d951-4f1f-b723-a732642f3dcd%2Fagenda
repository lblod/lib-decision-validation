            $('#overlaymenuDisclaimer').css('display', 'block');
            $('#loadingPanel').css('display', 'none');
        }

        function HideDisclaimer()
        {
            $('#overlaymenuDisclaimer').css('display', 'none');
            $('#overlay_master').css('display', 'none')
        }

